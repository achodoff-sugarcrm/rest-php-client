{
  "name": "sugarcrm/rest-php-client",
  "description": "An SDK for interacting with Sugar's REST v10 API.",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Mike Russell",
      "email": "mrussell@sugarcrm.com",
      "role": "Developer"
    }
  ],
  "keywords": [
    "sugarcrm",
    "sugar",
    "crm",
    "rest",
    "sdk",
    "v10",
    "api"
  ],
  "require": {
    "php": ">=7.2",
    "michaelj2324/php-rest-client": "^2.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "9.*",
    "phpdocumentor/phpdocumentor": "3.*",
    "squizlabs/php_codesniffer": "^3"
  },
  "autoload": {
    "psr-4": {
      "Sugarcrm\\REST\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sugarcrm\\REST\\Tests\\": "tests/"
    }
  }
}
